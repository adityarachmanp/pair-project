<%- include('partial/head')  %>



<div class="container">
    <div class="jumbotron">
      <h1>Selamat Datang <%= name  %> </h1>
      <p>Kami hadir untuk memenuhi kebutuhan harian anda</p>
    </div>

  </div>
<% if(logout) { %> 
   <h1>Halaman Ini Hanya bisa di akses Oleh Pengguna</h1>
<% } %> 
<div>

<% if(prod) { %>

    <table border="1px">
    
        <% prod.forEach(element => { %>
    
            <tr>
                <td><img src="<%= element.picture  %> " alt="<%= element.name  %> " style="width:104px;height:142px;"></td>
                <td>
                    Nama product : <%= element.name  %> <br>
                    Harga: <%= element.price  %> <br>
                    Category : <%= element.category %>  <br><br>
                    <button type="button">Tambahkan ke Keranjang</button>
                    <a href="/product/<%= element.id %>"> info lebih lanjut</a>
                
                </td>
            </tr>
            
       <%  }); %> 
    
    </table>
    
    </div>

<% } %> 


<%- include('partial/foot')  %> 
